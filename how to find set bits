#include<iostream>
using namespace std;

int setbits(int n){
    int k=0;
    while(n){
        int bit = n&1;
        if(bit){
            k++;
        }
        else{
            
        }
        n=n>>1;
    }
    return k;
}

int main(){
    int n;
    cin>>n;
    int a=setbits(n);
    cout<<a<<endl;
}
